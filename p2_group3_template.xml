<?xml version="1.0" encoding="UTF-8"?>
<?xml-model href="./schema_3302.rng" type="application/xml" schematypens="http://relaxng.org/ns/structure/1.0"?>
<!--the second line in the document associates the schema, so be sure not to change it-->
<DOC>
    <docHead>
        <!--required header includes metadata about the assignment (title, author, version)-->
        <title>Title of Assignment</title>
        <author xml:id="your_id_here">Your name here</author>
        <version n="num_of_version" date="2016-01-01"/>
        <!--note that the date must be YYYY-MM-DD for the document to be valid-->
    </docHead>

    <pub_init_proposal>
        <cover_page>
            <title> </title>
            <author> </author>
            <submit_date> </submit_date>
            <audience>
                <!--text here-->
            </audience>
        </cover_page>
        
        <div title="">
            <tbl_cont>
                <section n="">
                    <!--text here-->
                </section>
            </tbl_cont>
        </div>
        
        <div title="">
            <intro type="">  <!--attribute options: ltr_int, exec_sum, abstract-->
                <!--text here-->
            </intro>
        </div>
        
        <div title="Background">
            <background>
                <crnt_cond>
                    <!-- can include <q> element(s) for quoted material -->
                </crnt_cond>
                <subj_hist>
                    <!-- can include <q> element(s) for quoted material -->
                </subj_hist>
            </background>
        </div>
        
        <div title="">
            <purpose>
                <scope type=""> </scope>
                <objective>
                    <!-- can include <q> element(s) for quoted material -->
                </objective>
                <goal>
                    <!-- can include <q> element(s) for quoted material -->
                </goal>
            </purpose>
        </div>
        
        <div title="">
            <approach type="">  <!--attribute options: execution, action-->
                <!--text here-->
            </approach>
        </div>
        
        <div title="">
            <feasibility type="">  <!--attribute options: economic, social, environmental-->
                <!--text here-->
            </feasibility>
        </div>
        
        <div title="">
            <conclu>
                <!--text here-->
            </conclu>
        </div>
        
        <div title="">
            <references>
                <citation n="" style=""> </citation>
            </references>
        </div>
    </pub_init_proposal>

    <docReview>
        <reviewer type="instructor" xml:id="kgs"/>
        <report type="instructor" resp="#kgs">
            <p>Comments on assignment will appear here.</p>
        </report>
    </docReview>
</DOC>
