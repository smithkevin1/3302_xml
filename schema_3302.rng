        <zeroOrMore>
            <element name="intent">
                <attribute name="type">
                    <value>connection</value>
                </attribute>
                <ref name="macro.paraContent"/>
            </element>
        </zeroOrMore>
    </define>
    <define name="reason">
        <element name="reason">
            <ref name="macro.paraContent"/>
        </element>
    </define>
    <define name="proposition">
        <element name="proposition">
            <ref name="macro.paraContent"/>
        </element>
    </define>
    <define name="intro_para">
        <element name="intro_para">
            <interleave>
                <ref name="intent"/>
                <optional>
                    <ref name="reason"/>
                </optional>
                <ref name="proposition"/>
                <zeroOrMore>
                    <ref name="macro.paraContent"/>
                </zeroOrMore>
            </interleave>
        </element>
    </define>
    <define name="experience">
        <element name="experience">
            <ref name="macro.paraContent"/>
            <attribute name="type">
                <choice>
                    <value>work</value>
                    <value>personal</value>
                    <value>school</value>
                    <data type="token">
                        <param name="pattern">(\p{L}|\p{N}|\p{P}|\p{S})+</param>
                    </data>
                </choice>
            </attribute>
        </element>
    </define>
    <define name="para_background">
        <element name="para_background">
            <oneOrMore>
                <ref name="experience"/>
            </oneOrMore>
        </element>
    </define>
    <define name="compatability">
        <element name="compatability">
            <ref name="macro.paraContent"/>
        </element>
    </define>
    <define name="next_step">
        <element name="next_step">
            <ref name="macro.paraContent"/>
        </element>
    </define>
    <define name="para_closing">
        <element name="para_closing">
            <ref name="compatability"/>
            <ref name="next_step"/>
        </element>
    </define>
    <define name="salutation">
        <element name="salutation">
            <ref name="macro.paraContent"/>
        </element>
    </define>
    <define name="signature">
        <element name="signature">
            <ref name="salutation"/>
            <ref name="name"/>
        </element>
    </define>
    <define name="cover_letter">
        <element name="cover_letter">
            <ref name="header"/>
            <ref name="greeting"/>
            <ref name="intro_para"/>
            <oneOrMore>
                <ref name="para_background"/>
            </oneOrMore>
            <ref name="para_closing"/>
            <ref name="signature"/>
        </element>
    </define>
    <!-- end group 4 definitions -->

    <define name="docHead">
        <element name="docHead">
            <group>
                <interleave>
                    <ref name="title"/>
                    <ref name="author"/>
                    <oneOrMore>
                        <ref name="version"/>
                    </oneOrMore>
                    <zeroOrMore>
                        <ref name="model.global"/>
                    </zeroOrMore>
                </interleave>
            </group>


        </element>
    </define>
    <define name="DOC">
        <element name="DOC">
            <group>
                <ref name="docHead"/>
                <oneOrMore><choice>
                    <!--<ref name="body"/> 
                        not needed yet, if at all -->
                    <ref name="annotated_bib"/>
                    <ref name="blog_post"/>
                    <ref name="fsb_rpt"/>
                    <ref name="pub_init_proposal"/>
                    <ref name="cover_letter"/>
                </choice></oneOrMore>
                <optional>
                    <ref name="docFoot"/>
                </optional>
                <optional>
                    <ref name="docReview"/>
                </optional>
            </group>
            <ref name="att.global.attributes"/>
            <ref name="att.typed.attributes"/>
        </element>
    </define>

    <start>
        <ref name="DOC"/>
    </start>
</grammar>
